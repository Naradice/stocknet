{
    "size":0,
    "query": {
        "range":{
            "@timestamp": {
                "time_zone": "+09:00",
                "gte": "now-5h/m",
                "lt": "now/m"
            }
        }
    },
    "aggs": {
    "ticks": {
      "date_histogram": {
        "field": "@timestamp",
        "calendar_interval": "1m"
      },
      "aggs": {
            "low": { "min": { "field": "rate" } },
            "high": { "max": { "field": "rate" } },
            "first": {"top_hits": {
                "sort": [
                    {"@timestamp": {"order": "asc"}}
                ],
            "_source": {
              "includes": [ "rate" , "@timestamp"]
            },
            "size": 1
          }},
          "last": {"top_hits": {
                "sort": [
                    {"@timestamp": {"order": "desc"}}
                ],
            "_source": {
              "includes": [ "rate", "@timestamp" ]
            },
            "size": 1
          }}
        }
    }
  }
}